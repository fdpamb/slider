{
  "name": "rc-slider",
  "version": "9.7.5",
  "description": "Slider UI component for React",
  "engines": {
    "node": ">=8.x"
  },
  "keywords": [
    "react",
    "react-component",
    "react-slider",
    "slider",
    "input",
    "range"
  ],
  "homepage": "http://github.com/fdpamb/slider/",
  "repository": {
    "type": "git",
    "url": "git@github.com:fdpamb/slider.git"
  },
  "bugs": {
    "url": "http://github.com/fdpamb/slider/issues"
  },
  "files": [
    "assets/*.css",
    "lib",
    "es",
    "dist"
  ],
  "license": "MIT",
  "main": "./lib/index",
  "module": "./es/index",
  "style": "./assets/index.css",
  "types": "./lib/index.d.ts",
  "scripts": {
    "start": "dumi dev",
    "build": "npm run compile",
    "docs:build": "dumi build",
    "docs:deploy": "gh-pages -d .doc",
    "compile": "father build && lessc assets/index.less assets/index.css",
    "prepublishOnly": "npm run compile && np --yolo --no-publish",
    "lint": "eslint src/ --ext .ts,.tsx,.jsx,.js,.md",
    "prettier": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "test": "father test",
    "coverage": "father test --coverage",
    "now-build": "npm run docs:build"
  },
  "peerDependencies": {
    "react": ">=16.9.0",
    "react-dom": ">=16.9.0"
  },
  "dependencies": {
    "@babel/runtime": "^7.24.7",
    "classnames": "^2.5.1",
    "rc-tooltip": "github:fdpamb/tooltip#upgrade-5.x",
    "rc-util": "^5.43.0",
    "shallowequal": "^1.1.0"
  },
  "devDependencies": {
    "@types/classnames": "^2.3.1",
    "@types/jest": "^29.5.12",
    "@types/react": "^16.14.60",
    "@types/react-dom": "^18.3.0",
    "@umijs/fabric": "^4.0.1",
    "cross-env": "^7.0.3",
    "dumi": "^2.4.5",
    "enzyme": "^3.1.0",
    "enzyme-adapter-react-16": "^1.0.1",
    "enzyme-to-json": "^3.1.2",
    "eslint": "^9.6.0",
    "father": "^2.30.23",
    "father-build": "^1.18.6",
    "gh-pages": "^6.1.1",
    "glob": "^11.0.0",
    "less": "^3.13.1",
    "np": "^10.0.6",
    "rc-trigger": "github:fdpamb/trigger#upgrade-5.x",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "typescript": "^5.5.3"
  }
}
